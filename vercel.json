{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/pos.html",
      "dest": "/pos.html"
    },
    {
      "src": "/receipt.html",
      "dest": "/receipt.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
